        <llodia:record rdf:resource="r_mahapehá_1"/>
        <llodia:record rdf:resource="r_mahapehá_3"/>
        <llodia:record rdf:resource="r_mahapehá_4"/>
        <llodia:record rdf:resource="r_revolutio_1"/>
        <llodia:record rdf:resource="r_revolutio_2"/>
    </llodia:LexicalChronicle>
    

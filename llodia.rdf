<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:llodia="https://github.com/nexuslinguarum/llodia/"
    xml:base="https://github.com/nexuslinguarum/llodia/"
    xmlns:cc="http://creativecommons.org/ns#"
    xmlns:foaf="http://xmlns.com/foaf/spec/"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:dct="http://purl.org/dc/terms/"
    xmlns:dcmitype="http://purl.org/dc/dcmitype/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:ontolex="http://www.w3.org/ns/lemon/ontolex#"
    xmlns:lexinfo="http://www.lexinfo.net/ontology/2.0/lexinfo#"
    xmlns:lexicog="http://www.w3.org/ns/lemon/lexicog#"
    xmlns:frac="http://www.w3.org/ns/lemon/frac#"
    xmlns:cito="http://purl.org/spar/cito#"
    xmlns:void="http://rdfs.org/ns/void#"
    xmlns:time="http://www.w3.org/2006/time#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vartrans="http://www.w3.org/ns/lemon/vartrans#"
    xmlns:lemonEty="http://lari-datasets.ilc.cnr.it/lemonEty#"
    xmlns:dbo="http://dbpedia.org/ontology/">
    
    <owl:Ontology rdf:about="https://github.com/nexuslinguarum/llodia">
        <cc:license rdf:resource="https://www.gnu.org/licenses/agpl-3.0.html"/>
        <dc:creator rdf:ID="far"><foaf:Person><foaf:name>Florentina Armaselu</foaf:name><foaf:workInfoHomepage rdf:resource="https://orcid.org/0000-0003-2386-6889"/></foaf:Person></dc:creator>
        <dc:contributor rdf:ID="cli"><foaf:Person><foaf:name>Chaya Liebeskind</foaf:name><foaf:workInfoHomepage rdf:resource="https://orcid.org/0000-0003-0476-3796"/></foaf:Person></dc:contributor>
        <dc:contributor rdf:ID="cot"><foaf:Person><foaf:name>Ciprian-Octavian Truică</foaf:name><foaf:workInfoHomepage rdf:resource="https://orcid.org/0000-0001-7292-4462"/></foaf:Person></dc:contributor>
        <dc:contributor rdf:ID="gvo"><foaf:Person><foaf:name>Giedrė Valūnaitė Oleškevičienė</foaf:name><foaf:workInfoHomepage rdf:resource="https://orcid.org/0000-0001-5688-2469"/></foaf:Person></dc:contributor>
        <dc:contributor rdf:ID="pma"><foaf:Person><foaf:name>Paola Marongiu</foaf:name><foaf:workInfoHomepage rdf:resource="https://orcid.org/0000-0002-5060-3307"/></foaf:Person></dc:contributor>
        <dc:contributor rdf:ID="bmc"><foaf:Person><foaf:name>Barbara McGillivray</foaf:name><foaf:workInfoHomepage rdf:resource="https://orcid.org/0000-0003-3426-8200"/></foaf:Person></dc:contributor>
	<dc:contributor rdf:ID="esa"><foaf:Person><foaf:name>Elena Simona Apostol</foaf:name><foaf:workInfoHomepage rdf:resource="https://orcid.org/0000-0001-6397-4951"/></foaf:Person></dc:contributor>
        <rdfs:comment xml:lang="eng">Contribution: F.A. created the ontology, the examples for French and contributed to the harmonisation of the examples for all languages; C.L. contributed the examples for Hebrew; G.V.O. contributed the examples for Lithuanian; C.O.T. and E.S.A. contributed the examples for Romanian; P.M and B.McG. contributed the examples for Latin.</rdfs:comment>
        <dc:description xml:lang="en">A humanities use case that models lexico-semantic data extracted from diachronic corpora and dictionaries as Linguistic Linked Open Data (LLOD).</dc:description>
        <dc:title xml:lang="en">LLOD Modelling in Diachronic Analysis</dc:title>
    </owl:Ontology>
    
    
    <!-- Classes -->
    <owl:Class rdf:about="https://github.com/nexuslinguarum/llodia/LexicalChronicle">
        <rdfs:subClassOf rdf:resource="http://rdfs.org/ns/void#Dataset"/>
        <dc:description>A collection of lexical diachronic records.</dc:description>    
    </owl:Class>
    
    <owl:Class rdf:about="https://github.com/nexuslinguarum/llodia/LexicalRecord">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/lemon/frac#Observable"/>
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A record about a form, which includes temporal information indicating when a certain linguistic event about the form was observed.</dc:description>
    </owl:Class>
    
    <owl:Class rdf:about="https://github.com/nexuslinguarum/llodia/Dictionary">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/lemon/lexicog#LexicographicResource"/>
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A lexicographic resource that can be used as a reference about word forms, meanings, usage, etc.</dc:description>    
    </owl:Class>
    
    <owl:Class rdf:about="https://github.com/nexuslinguarum/llodia/Corpus">
        <rdfs:subClassOf rdf:resource="http://purl.org/dc/dcmitype/Collection"/>
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A collection of documents that covers a certain topic and period of time.</dc:description>    
    </owl:Class>
    
    <owl:Class rdf:about="https://github.com/nexuslinguarum/llodia/TimeInterval">
        <rdfs:subClassOf rdf:resource="http://purl.org/dc/terms/Period"/>
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A class that defines a time interval. It may also be an open interval or include non-numeric encoding referring, for instance, to a named era.</dc:description>
        <owl:DatatypeProperty rdf:resource="dct:start"/>
        <owl:DatatypeProperty rdf:resource="dct:end"/>
    </owl:Class>
    
    
    <!-- Properties -->
    <!-- DatatypeProperty -->
    <owl:DatatypeProperty rdf:about="https://github.com/nexuslinguarum/llodia/publicationDate">
        <rdfs:subPropertyOf rdf:resource="dct:date"/>
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A property that relates a publication date with a corpus or dictionary.</dc:description>
    </owl:DatatypeProperty>
    
    <!-- ObjectProperty -->
    <owl:ObjectProperty rdf:about="https://github.com/nexuslinguarum/llodia/timeSpan">
        <rdfs:subPropertyOf rdf:resource="dct:coverage"/>
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A property that defines the time span of a resource.</dc:description>
        <rdfs:range rdf:resource="https://github.com/nexuslinguarum/llodia/TimeInterval"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="https://github.com/nexuslinguarum/llodia/timeSlice">
        <rdfs:subPropertyOf rdf:resource="dct:coverage"/>
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A property that relates a record with the time slice in which a specific linguistic event was observed.</dc:description>
        <rdfs:domain rdf:resource="https://github.com/nexuslinguarum/llodia/LexicalRecord"/>
        <rdfs:range rdf:resource="https://github.com/nexuslinguarum/llodia/TimeInterval"/>
        <owl:inverseOf rdf:resource="https://github.com/nexuslinguarum/llodia/isTimeSliceOf"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="https://github.com/nexuslinguarum/llodia/isTimeSliceOf">
        <rdfs:subPropertyOf rdf:resource="dct:coverage"/>
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A property that relates a time slice when a specific linguistic event was observed with a record.</dc:description>
        <rdfs:domain rdf:resource="https://github.com/nexuslinguarum/llodia/TimeInterval"/>
        <rdfs:range rdf:resource="https://github.com/nexuslinguarum/llodia/LexicalRecord"/>
        <owl:inverseOf rdf:resource="https://github.com/nexuslinguarum/llodia/timeSlice"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="https://github.com/nexuslinguarum/llodia/record">
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A property that relates a lexical chronicle to a lexical record.</dc:description>
        <rdfs:domain rdf:resource="https://github.com/nexuslinguarum/llodia/LexicalChronicle"/>
        <rdfs:range rdf:resource="https://github.com/nexuslinguarum/llodia/LexicalRecord"/>
        <owl:inverseOf rdf:resource="https://github.com/nexuslinguarum/llodia/isRecordOf"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="https://github.com/nexuslinguarum/llodia/isRecordOf">
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A property that relates a lexical record to a lexical chronicle.</dc:description>
        <rdfs:domain rdf:resource="https://github.com/nexuslinguarum/llodia/LexicalRecord"/>
        <rdfs:range rdf:resource="https://github.com/nexuslinguarum/llodia/LexicalChronicle"/>
        <owl:inverseOf rdf:resource="https://github.com/nexuslinguarum/llodia/record"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="https://github.com/nexuslinguarum/llodia/form">
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A property that relates a lexical record to a form.</dc:description>
        <rdfs:domain rdf:resource="https://github.com/nexuslinguarum/llodia/LexicalRecord"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/lemon/ontolex#Form"/>
        <owl:inverseOf rdf:resource="https://github.com/nexuslinguarum/llodia/isFormOf"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="https://github.com/nexuslinguarum/llodia/isFormOf">
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A property that relates a form to a lexical record.</dc:description>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/lemon/ontolex#Form"/>
        <rdfs:range rdf:resource="https://github.com/nexuslinguarum/llodia/LexicalRecord"/>
        <owl:inverseOf rdf:resource="https://github.com/nexuslinguarum/llodia/form"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="https://github.com/nexuslinguarum/llodia/similarity">
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A property that relates the characteristics of a similarity-based observation with a container of observed items, such as a list of neighbours.</dc:description>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/lemon/frac#Observation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Container"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="https://github.com/nexuslinguarum/llodia/lexicalConcept">
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A property that relates a lexical record with a lexical concept.</dc:description>
        <rdfs:domain rdf:resource="https://github.com/nexuslinguarum/llodia/LexicalRecord"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/lemon/ontolex#LexicalConcept"/>
        <owl:inverseOf rdf:resource="https://github.com/nexuslinguarum/llodia/isLexicalConceptOf"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="https://github.com/nexuslinguarum/llodia/isLexicalConceptOf">
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A property that relates a lexical concept with a lexical record.</dc:description>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/lemon/ontolex#LexicalConcept"/>
        <rdfs:range rdf:resource="https://github.com/nexuslinguarum/llodia/LexicalRecord"/>
        <owl:inverseOf rdf:resource="https://github.com/nexuslinguarum/llodia/lexicalConcept"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="https://github.com/nexuslinguarum/llodia/translationSet">
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A property that relates a form or a lexical sense with a target translation set.</dc:description>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/ns/lemon/ontolex#Form"/>
                    <rdf:Description rdf:about="http://www.w3.org/ns/lemon/ontolex#LexicalSense"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/ns/lemon/vartrans#TranslationSet"/>
        <owl:inverseOf rdf:resource="https://github.com/nexuslinguarum/llodia/isTranslationSetOf"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="https://github.com/nexuslinguarum/llodia/isTranslationSetOf">
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A property that relates a translation set with a form or a lexical sense.</dc:description>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/lemon/vartrans#TranslationSet"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/ns/lemon/ontolex#Form"/>
                    <rdf:Description rdf:about="http://www.w3.org/ns/lemon/ontolex#LexicalSense"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <owl:inverseOf rdf:resource="https://github.com/nexuslinguarum/llodia/translationSetOf"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="https://github.com/nexuslinguarum/llodia/etymon">
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A property that relates an etymon form or a lexical sense with an etymology.</dc:description>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/ns/lemon/ontolex#Form"/>
                    <rdf:Description rdf:about="http://www.w3.org/ns/lemon/ontolex#LexicalSense"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://lari-datasets.ilc.cnr.it/lemonEty#Etymology"/>
        <owl:inverseOf rdf:resource="https://github.com/nexuslinguarum/llodia/isEtymonOf"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="https://github.com/nexuslinguarum/llodia/isEtymonOf">
        <rdfs:isDefinedBy rdf:resource="https://github.com/nexuslinguarum/llodia"/>
        <dc:description>A property that relates an etymology with an etymon form or lexical sense.</dc:description>
        <rdfs:domain rdf:resource="http://lari-datasets.ilc.cnr.it/lemonEty#Etymology"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/ns/lemon/ontolex#Form"/>
                    <rdf:Description rdf:about="http://www.w3.org/ns/lemon/ontolex#LexicalSense"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <owl:inverseOf rdf:resource="https://github.com/nexuslinguarum/llodia/etymon"/>
    </owl:ObjectProperty>
    
    <!-- Lexical chronicle -->
    <llodia:LexicalChronicle rdf:about="lc_nexus">

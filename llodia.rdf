    <!-- Lexical senses -->
    <ontolex:LexicalSense rdf:about="d_plex_fra_révolution_n_I.B.2">
        <ontolex:reference rdf:resource="d_plex_fra"/>
        <rdfs:comment xml:lang="eng">Circular motion of a body around its axis.</rdfs:comment>
        <dct:subject rdf:resource="https://dbpedia.org/page/Category:Mechanics"/>
        <dct:source rdf:resource="https://www.cnrtl.fr/definition/r%C3%A9volution"/>
        <frac:attestation rdf:resource="da_révolution_1"/>
    </ontolex:LexicalSense>
    
    <ontolex:LexicalSense rdf:about="d_plex_fra_révolution_n_I.B.1">
        <ontolex:reference rdf:resource="d_plex_fra"/>
        <rdfs:comment xml:lang="eng">Motion of a geometric form around an axis.</rdfs:comment>
        <dct:subject rdf:resource="https://dbpedia.org/page/Category:Geometry"/>
        <dct:source rdf:resource="https://www.cnrtl.fr/definition/r%C3%A9volution"/>
        <frac:attestation rdf:resource="da_révolution_2"/>
    </ontolex:LexicalSense>
    
